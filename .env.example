# Supabase Configuration (New Key Format)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=sb_publishable_your-publishable-key-here
SUPABASE_SECRET=sb_secret_your-secret-key-here

# Optional: Direct PostgreSQL connection (for migrations)
# DATABASE_URL=postgresql://postgres:your-password@aws-0-us-east-1.pooler.supabase.com:5432/postgres
# SUPABASE_DB_PASSWORD=your-database-password
# SUPABASE_POOLER_HOST=aws-0-us-east-1.pooler.supabase.com

# Anthropic API Configuration
ANTHROPIC_API_KEY=sk-ant-api03-...your-key-here
ANTHROPIC_MODEL=claude-opus-4-5

# Ralph Configuration
RALPH_TIMEOUT_MINUTES=30
RALPH_QUALITY_THRESHOLD=0.85
RALPH_QUALITY_FLOOR=0.70
RALPH_COST_LIMIT_CENTS=100

# Email Alerts (Resend.com)
EMAIL_PROVIDER=resend
RESEND_API_KEY=re_xxxxxxxxxx
EMAIL_FROM=onboarding@resend.dev
EMAIL_TO=your-email@example.com
